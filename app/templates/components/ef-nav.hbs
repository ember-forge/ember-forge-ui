{{#if hasBlockParams}}
  {{#ef-list
    usedAs="nav"
  }}
    {{yield (hash

      divider=(component
        "ef-list-divider"
        usedAs="nav"
      )

      group-header=(component
        "ef-list-group-header"
        usedAs="nav"
      )

      header=(component
        "ef-list-header"
        usedAs="nav"
      )

      item=(component
        "ef-list-item"
        active=active
        disabled=disabled
        usedAs="nav"
      )
    )}}
  {{/ef-list}}
{{else}}
  You must use block params (<code>as | |</code>) when using the <code>ef-nav</code> component.
{{/if}}
