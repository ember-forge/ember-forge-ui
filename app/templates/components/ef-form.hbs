{{yield (hash
  fieldset=(component "form/ef-fieldset")
  input-hidden=(component "form/input/ef-hidden" data=data)
  input-text=(component "form/input/ef-text" data=data)
  label=(component "form/ef-label")
  legend=(component "form/ef-legend")
  reset=(component "ef-button" type="reset")
  submit=(component "ef-button" type="submit")
  textarea=(component "form/ef-textarea" data=data)
)}}
