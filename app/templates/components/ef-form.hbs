{{yield (hash
  fieldset=(component "form/ef-fieldset")
  input-date=(component "form/input/ef-date" data=data)
  input-email=(component "form/input/ef-email" data=data)
  input-hidden=(component "form/input/ef-hidden" data=data)
  input-month=(component "form/input/ef-month" data=data)
  input-password=(component "form/input/ef-password" data=data)
  input-search=(component "form/input/ef-search" data=data)
  input-telephone=(component "form/input/ef-telephone" data=data)
  input-text=(component "form/input/ef-text" data=data)
  input-time=(component "form/input/ef-time" data=data)
  input-url=(component "form/input/ef-url" data=data)
  input-week=(component "form/input/ef-week" data=data)
  label=(component "form/ef-label")
  legend=(component "form/ef-legend")
  reset=(component "ef-button" type="reset")
  submit=(component "ef-button" type="submit")
  textarea=(component "form/ef-textarea" data=data)
)}}
